{
  "name": "elm-to-react",
  "version": "0.1.0",
  "private": true,
  "description": "A source-to-source transpiler that converts Elm projects into TypeScript/React code",
  "repository": {
    "type": "git",
    "url": "https://github.com/bentefay/elm-to-react.git"
  },
  "author": "Ben Tefay",
  "license": "MIT",
  "type": "module",
  "bin": "./dist/main.js",
  "packageManager": "yarn@4.10.3",
  "scripts": {
    "dev": "node --experimental-strip-types src/main.ts",
    "build": "tsc",
    "gen:cst-types": "node ./node_modules/@asgerf/dts-tree-sitter/build/src/index.js @elm-tooling/tree-sitter-elm > src/parser/cst-types/index.d.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@effect/cli": "0.71.0",
    "@effect/cluster": "0.50.6",
    "@effect/experimental": "0.56.0",
    "@effect/platform": "0.92.1",
    "@effect/platform-node": "0.98.4",
    "@effect/printer": "0.46.0",
    "@effect/printer-ansi": "0.46.0",
    "@effect/rpc": "0.71.1",
    "@effect/sql": "0.46.0",
    "@effect/typeclass": "0.37.0",
    "@effect/workflow": "0.11.5",
    "effect": "3.18.4"
  },
  "devDependencies": {
    "@asgerf/dts-tree-sitter": "0.21.0",
    "@effect/eslint-plugin": "0.3.2",
    "@elm-tooling/tree-sitter-elm": "5.8.0",
    "@eslint/js": "^9.38.0",
    "@types/node": "24.8.1",
    "@typescript-eslint/eslint-plugin": "8.46.1",
    "@typescript-eslint/parser": "8.46.1",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "prettier": "3.6.2",
    "tree-sitter": "0.25.0",
    "tree-sitter-cli": "0.25.10",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "dependenciesMeta": {
    "@elm-tooling/tree-sitter-elm": {
      "built": true
    },
    "@parcel/watcher": {
      "built": true
    },
    "esbuild": {
      "built": true
    },
    "tree-sitter": {
      "built": true
    },
    "tree-sitter-cli": {
      "built": true
    }
  }
}
